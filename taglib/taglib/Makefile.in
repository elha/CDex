libtag_la.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/tag.cpp $(srcdir)/fileref.cpp $(srcdir)/audioproperties.cpp 
	@echo 'creating libtag_la.all_cpp.cpp ...'; \
